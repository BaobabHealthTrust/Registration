<html>
<head>
<%= javascript_include_tag "prototype" %>
<%= stylesheet_link_tag "cohort" %>
<script>
  var tt_cancel_destination = "/"  
</script>
<style>
  table {
    border-style:none;
    border-width:1px;
  }

  th {
    border-style:none;
    border-width:1px;
  }

  td {
    text-align:center;
    border-style:solid;
    border-width:1px;
  }

  .row_break td {
    border-width:0px;
  }

  div.scrollTableContainer {
    border-style:solid;
  }

  .survival-analysis th, .survival-analysis td {
    text-align:left;
  }
</style>

<title>Cohort<%= @location + "  " + @report_type %></title>
</head>

